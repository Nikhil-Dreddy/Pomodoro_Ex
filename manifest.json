{

  "manifest_version": 2,
  "name": "Pomodoro",
  "version": "1.0",

  "description": "Used to help users study",

  "icons": {
    "48": "icons/tomato.png"
  },

   "browser_action": {
      "default_icon": {
        "48": "icons/tomato.png"
      },
      "default_title": "Study"
    },
    
  "content_scripts": [
    {
      "matches": ["*://*.reddit.com/*"],
      "js": ["Pomodoro.js"]
    }
  ]

}